{
	"name": "marttiphpbb/templateevents",
	"type": "phpbb-extension",
	"description": "This extension for developers shows all template events",
	"homepage": "https://github.com/marttiphpbb/phpbb-ext-templateevents",
	"version": "0.1.1",
	"time": "2014-11-15",
	"keywords": ["phpbb", "extension", "template", "events"],
	"license": "MIT",
	"authors": [{
			"name": "martti",
			"email": "info@martti.be",
			"homepage": "https://github.com/marttiphpbb/",
			"role": "Developer"
		}],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Template Events",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0,<3.2.*@dev"
		},
		"version-check": {
			"host": "marttiphpbb.github.io",
			"directory": "/version",
			"filename": "templateevents.json"
		}
	}
}
